  "private": true,
  "dependencies": {
    "apollo-client": "^1.0.0-rc.6",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "functional-easing": "^1.0.8",
    "immutability-helper": "^2.1.2",
